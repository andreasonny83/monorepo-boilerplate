{
  "name": "@my-app",
  "description": "Sample Monorepo",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:client": "pnpm --filter \"@my-app/client\" start",
    "start:api": "pnpm --filter \"@my-app/api\" start",
    "start": "concurrently \"pnpm run start:client\" \"pnpm run start:api\""
  },
