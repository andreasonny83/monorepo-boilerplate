  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "concurrently": "^9.0.1",
    "eslint": "^9.12.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  }
}
